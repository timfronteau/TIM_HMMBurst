lags = np.arange(-9, 10) i.e. [-9, -8, ..., 0, ..., 8, 9]  # Time-delay embedding
trial = 10           # Number of trials we concatenate to infer a model
n_components = 8     # For the principal component analysis
n_states = 3         # for the Hidden Markov Model

covariance_type = 'diag'    # Type of the inferred covariance matrix : delays*IC params are inferred
model_type = 'GaussianHMM'  # Observation model (here, a means-array and a covariance-matrix is inferred)
n_iter = 100
tol = 0.01
downsamp_rate = 1 (there is no downsampling)

|_ (19delays)  x (7s x 256Hz x 10trials)
            -----PCA-----> |_ (8components) x (7s x 256Hz x 10trials) 
            -------------> TDE-HMM ( 1 Gaussian obs. model per state, 3 states )
